<div class="recent">
    <div class="activity">
        <h1>Recent Activity</h1>
    </div>
    <div class="personHead">
        <p>Name</p>
        <p>Balance</p>
        <p>Company</p>
        <p>Contact</p>
    </div>
    <% data.forEach((item)=>{%>
        <div class="person">
            <p><%=item.name%></p>
            <p><%=item.balance%></p>
            <p><%=item.company%></p>
            <p><%=item.email%></p>
        </div>
    <%})%>
    <div>

    </div>
</div>